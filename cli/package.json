{
  "name": "@kazuph/npmstat-cli",
  "version": "1.0.0",
  "description": "Advanced CLI tool for comprehensive NPM package statistics and analytics",
  "keywords": ["npm", "statistics", "downloads", "analytics", "cli", "package", "stats"],
  "author": "kazuph <kazuph@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kazuph/npm-stats-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazuph/npm-stats-cli.git"
  },
